@model string
@if (!String.IsNullOrEmpty(Model))
{
    if(Model.StartsWith("Error")){
        <div class="alert alert-danger alert-dismissible" role="alert">
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
            @Model
        </div>
    }
    else{
        <div class="alert alert-success alert-dismissible" role="alert">
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
            <h2 class="fw-bold text-center " style="color: #FF6F00">NHẤN VÀO BÊN DƯỚI ĐỂ TẢI KEY XUỐNG</h2>
            <hr />
            <div class="form-label d-flex justify-content-center">
                <a class="text-center fw-bold mb-2" style="text-decoration: none; color:#FF6F00;" href="data:text/plain;charset=utf-8,@Model" download="Key.txt">Tải key</a>
            </div>
        </div>
    }
}

